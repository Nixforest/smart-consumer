        <action path="/deleteComment" type="com.gae.java.smartconsumer.mobile.action.DeleteCommentById" scope="session" validate="false"></action>
        <action path="/getListCategories" type="com.gae.java.smartconsumer.mobile.action.GetListCategories" scope="session" validate="false"></action>
